{% extends 'beaglebone-black.jinja2' %}
{% set power_off_command = 'pduclient --daemon localhost --hostname drawer6 --command off --port 01 --delay 2' %}
{% set hard_reset_command = 'echo reboot' %}
{% set connection_command = '/usr/local/connect_forever localhost 6061' %}
{% set power_on_command = 'pduclient --daemon localhost --hostname drawer6 --command on --port 01 --delay 2' %}
