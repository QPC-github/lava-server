{% extends 'base-uboot.jinja2' %}
{% set console_device = console_device|default('ttyS0') %}
{% set baud_rate = baud_rate|default(115200) %}
{% set device_type = "armada-7040-db" %}
{% set bootloader_prompt = bootloader_prompt|default('Marvell>>') %}
{% set booti_kernel_addr  = '0x05000000' %}
{% set booti_ramdisk_addr = '0x0a000000' %}
{% set booti_dtb_addr     = '0x04f00000' %}
{% set base_ip_args = 'ip=dhcp' %}
{% set uboot_mkimage_arch = 'arm64' %}
{# Hack until mainline/next have the fix for CMA pool size #}
{% set extra_kernel_args = " cma=32M " %}
